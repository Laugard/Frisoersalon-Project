    <items>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0" styleClass="left-form">
            <children>
                <TableView fx:id="SletMedarbejderTabel" layoutX="478.0" layoutY="186.0" prefHeight="426.0" prefWidth="555.0">
                    <columns>
                        <TableColumn fx:id="fornavnColumn" prefWidth="103.0" text="Fornavn" />
                        <TableColumn fx:id="efternavnColumn" prefWidth="103.0" text="Efternavn" />
                        <TableColumn fx:id="emailColumn" prefWidth="348.0" text="Email" />
                    </columns>
                </TableView>
                <Button fx:id="SletMedarbejderBtn" layoutX="297.0" layoutY="357.0" mnemonicParsing="false" onAction="#handleSletMedarbejderAction" text="Slet medarbejder" />
            <Button fx:id="tilbageTilForsideBtn" layoutX="297.0" layoutY="523.0" mnemonicParsing="false" onAction="#tilbageTilForside" text="Tilbage til forsiden" />
            </children>
        </AnchorPane>
